{
  "name": "material-ui-form-helpers",
  "version": "0.1.12",
  "description": "Render helpers for material ui inputs",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack  --mode=production",
    "build:dev": "webpack --mode=development",
    "storybook": "start-storybook -p 9001"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/.jest/registerContext.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-datepicker).+\\.js$",
      "/node_modules/(?!reac-datepicker).+\\.css$"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurentiu1981/material-ui-form-helpers.git"
  },
  "keywords": [
    "MaterialUI",
    "forms",
    "inputs",
    "render"
  ],
  "author": "Laurentiu Lese",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-storyshots": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@testing-library/react": "^9.1.4",
    "@tulipjs/eslint-config": "^1.1.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-require-context-hook": "^1.0.0",
    "css-loader": "^2.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "node-sass": "^4.11.0",
    "react-test-renderer": "^16.9.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.4.1",
    "@material-ui/icons": "^4.4.1",
    "@material-ui/styles": "^4.4.1",
    "class-autobind": "^0.1.4",
    "clsx": "^1.0.4",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-color": "^2.17.3",
    "react-datepicker": "^2.8.0",
    "react-dom": "^16.8.5",
    "react-redux": "^5.1.1",
    "react-router": "^5.0.0",
    "react-select": "^2.4.4",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-form-storybook": "^1.0.2"
  },
  "bugs": {
    "url": "https://github.com/laurentiu1981/material-ui-form-helpers/issues"
  },
  "homepage": "https://github.com/laurentiu1981/material-ui-form-helpers#readme"
}
